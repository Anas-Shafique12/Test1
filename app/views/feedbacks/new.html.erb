<h1>New Feedback</h1>

<%= form_with model: @feedback, url: course_feedbacks_path(@course), local: true do |form| %>
  <div>
    <%= form.label :content %>
    <%= form.text_area :content %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>

<%= link_to 'Back', course_feedbacks_path(@course) %>
